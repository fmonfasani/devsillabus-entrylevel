    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "tailwindcss": "^3.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.19.14",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.2.7",
    "dotenv": "^17.2.2",
